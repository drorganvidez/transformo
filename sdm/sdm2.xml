<?xml version="1.0" encoding="utf-8" standalone="yes" ?>

<sdm>

    <entity id="post">
        <name>Post</name>
        <attribute>
            <name>title</name>
            <type>varchar(25)</type>
        </attribute>
        <attribute>
            <name>body</name>
            <type>text</type>
        </attribute>
    </entity>

    <entity id="post_meta">
        <name>PostMeta</name>
        <attribute>
            <name>author</name>
            <type>varchar(25)</type>
        </attribute>
        <attribute>
            <name>attribute_move</name>
            <type>text</type>
        </attribute>
    </entity>

    <entity id="comment">
        <name>comment</name>
        <attribute>
            <name>title</name>
            <type>varchar(25)</type>
        </attribute>
    </entity>

    <entity id="comment2">
        <name>comment</name>
        <attribute>
            <name>author2</name>
            <type>tipo1</type>
        </attribute>
    </entity>

    <relation>
        <one id="post"></one>
        <many id="post_meta"></many>
    </relation>

</sdm>