<?xml version="1.0" encoding="utf-8" standalone="yes" ?>

<sdm>

    <entity id="post">
        <name>Post</name>
        <attribute>
            <name>title</name>
            <type>String</type>
        </attribute>
        <attribute>
            <name>body</name>
            <type>String</type>
        </attribute>
        <attribute>
            <name>author</name>
            <type>String</type>
        </attribute>
        <attribute>
            <name>renombrado</name>
            <type>tipado</type>
        </attribute>
        <attribute>
            <name>nuevo</name>
            <type>tipado2</type>
        </attribute>
    </entity>

    <entity id="comment">
        <name>Comment</name>
        <attribute>
            <name>title</name>
            <type>varchar(100)</type>
        </attribute>
        <attribute>
            <name>comment</name>
            <type>text</type>
        </attribute>
    </entity>

    <entity id="nueva">
        <name>Nueva</name>
        <attribute>
            <name>title</name>
            <type>varchar(50)</type>
        </attribute>
        <attribute>
            <name>other</name>
            <type>number</type>
        </attribute>
    </entity>

    <relation>
        <one id="post"></one>
        <many id="comment"></many>
    </relation>

</sdm>