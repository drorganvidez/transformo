<?xml version="1.0" encoding="utf-8" standalone="yes" ?>

<sdbm>

    <entity id="post">
        <name>Post</name>
        <attribute>
            <name>title</name>
            <type>String</type>
        </attribute>
        <attribute>
            <name>body</name>
            <type>String</type>
        </attribute>
    </entity>

    <entity id="post_meta">
        <name>PostMeta</name>
        <attribute>
            <name>author</name>
            <type>String</type>
        </attribute>
    </entity>

    <relation>
        <many id="post"></many>
        <one id="post_meta"></one>
    </relation>

</sdbm>