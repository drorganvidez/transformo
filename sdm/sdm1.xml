<?xml version="1.0" encoding="utf-8" standalone="yes" ?>

<sdm>

    <entity id="post">
        <name>Post</name>
        <attribute>
            <name>title</name>
            <type>varchar(25)</type>
        </attribute>
        <attribute>
            <name>body</name>
            <type>text</type>
        </attribute>
        <attribute>
            <name>date</name>
            <type>date</type>
        </attribute>
        <attribute>
            <name>renombrar1</name>
            <type>tipado</type>
        </attribute>
        <attribute>
            <name>renombrar2</name>
            <type>tipado</type>
        </attribute>
        <attribute>
            <name>nuevo</name>
            <type>tipado1</type>
        </attribute>
    </entity>

    <entity id="post_meta">
        <name>PostMeta</name>
        <attribute>
            <name>author</name>
            <type>varchar(25)</type>
        </attribute>
    </entity>

    <entity id="vieja">
        <name>Vieja</name>
        <attribute>
            <name>title</name>
            <type>varchar(50)</type>
        </attribute>
        <attribute>
            <name>other</name>
            <type>number</type>
        </attribute>
    </entity>

    <entity id="borrar">
        <name>Borrar</name>
        <attribute>
            <name>title</name>
            <type>varchar(50)</type>
        </attribute>
    </entity>

    <relation>
        <one id="post"></one>
        <many id="post_meta"></many>
    </relation>

</sdm>